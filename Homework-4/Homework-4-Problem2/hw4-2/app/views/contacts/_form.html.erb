<%= form_for(@contact) do |f| %>
	<div>
		<%= label_tag "name_first", "First Name: " %>
		<%#= f.label(:name_first) %>
		<%= f.text_field(:name_first, :autofocus=>true) %>
	</div>
	<div>
		<%= label_tag "name_last", "Last Name: " %>
		<%#= f.label(:name_last) %>
		<%= f.text_field(:name_last) %>
	</div>
	<div>
		<%= label_tag "email", "email: " %>
		<%#= f.label(:email) %>
		<%= f.text_field(:email) %>
	</div>
	<div>
		<%= label_tag "message", "Message: " %>
		<%#= f.label(:message) %>
		<%= f.text_area(:message) %>
	</div>
	<br>
	<div>
		<%= f.submit %>
	</div>
<% end %>
